   "source": [
    "def get_odds():\n",
    "  for num in range(10):\n",
    "   if num % 2 != 0:\n",
    "      yield num\n",
    "\n",
    "odd_numbers = get_odds()\n",
    "for _ in range(3):\n",
    "    third_value = next(odd_numbers)\n",
    "\n",
    "print(third_value)"
